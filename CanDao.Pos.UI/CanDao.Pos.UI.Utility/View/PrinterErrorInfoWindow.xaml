<controls:PosMsgWindow x:Class="CanDao.Pos.UI.Utility.View.PrinterErrorInfoWindow"
                       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:controls="clr-namespace:CanDao.Pos.Common.Controls;assembly=CanDao.Pos.Common"
                       Height="200" Width="480" FontSize="24" CancelBtnText="">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="TbInfo" TextWrapping="Wrap"/>
        <CheckBox x:Name="CbNoWarning" Grid.Row="1" VerticalAlignment="Center"/>
    </Grid>
</controls:PosMsgWindow>
