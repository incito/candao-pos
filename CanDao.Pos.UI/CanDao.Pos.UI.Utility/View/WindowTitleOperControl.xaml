<UserControl x:Class="CanDao.Pos.UI.Utility.View.WindowTitleOperControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/CanDao.Pos.Common;component/BaseDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <dxlc:LayoutGroup VerticalAlignment="Top" Background="LightSlateGray">
        <Image Source="{StaticResource CanDaoPosImageLog2}" HorizontalAlignment="Left" Width="270"/>
        <Button HorizontalAlignment="Right" Style="{StaticResource BtnReturnStyle}" Width="64" Height="64" Command="{Binding CancelCmd}"/>
    </dxlc:LayoutGroup>
</UserControl>
