<Window x:Class="CanDaoCD.Pos.Common.Controls.CSystem.WUserCheck"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WUserCheck" Height="310" Width="410" Background="#F0F0F0" WindowStyle="None" AllowsTransparency="True">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40*"></RowDefinition>
            <RowDefinition Height="20*"></RowDefinition>
            <RowDefinition Height="20*"></RowDefinition>
            <RowDefinition Height="20*"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Text="会员注销权限验证" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="30"></TextBlock>
        <TextBox Grid.Row="1"></TextBox>
        <TextBox Grid.Row="2"></TextBox>
        <Button x:Name="BtnOk" Style="{DynamicResource ColBtn}" Grid.Row="3" Width="190" HorizontalAlignment="Left" Margin="5" Content="确定" Click="BtnOk_OnClick"></Button>
        <Button x:Name="BtnCancel" Style="{DynamicResource ColBtn}" Grid.Row="3" Width="190" HorizontalAlignment="Right" Margin="5" Content="取消" Click="BtnCancel_OnClick"></Button>
    </Grid>
</Window>
