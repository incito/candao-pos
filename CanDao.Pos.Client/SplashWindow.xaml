<Window x:Class="CanDao.Pos.Client.SplashWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Height="768" Width="1024"
        WindowStartupLocation="CenterScreen" WindowStyle="None"
        Padding="0">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/CanDao.Pos.Common;component/BaseDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Image Source="{StaticResource CanDaoPosSplashImg}" VerticalAlignment="Stretch" Stretch="Fill"/>
</Window>
